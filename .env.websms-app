# Letexto API base url, ex: https://<letexto-api-url>/v1
LETEXTO_API_URL=http://dummy

# MinIO server credentials. It is used to store files.
MINIO_HOST=websms-minio
MINIO_PORT=9000
# MinIO credentials. Should be the same as MINIO_ROOT_USER and MINIO_ROOT_PASSWORD respectively.
MINIO_ACCESS_KEY=admin1
MINIO_SECRET_KEY=admintrex18
# App's bucket name
MINIO_BUCKET=websms
MINIO_URL=http://websms-minio:9000

# Default log level. See winstonjs docs for more info.
LOG_LEVEL=info

# Amount of messages to take for each ongoing campaign when sending. Edit to suit your needs
MESSAGE_COUNT=100
